{
  "name": "tech-challenge-fiap",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npx prisma generate && npx prisma db push && npm run start",
    "start": "node ./src/server.js",
    "dev": "nodemon ./src/server.js",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "json-summary",
      "html"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 30
      }
    }
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "express-validator": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@prisma/client": "^5.17.0",
    "eslint": "^9.6.0",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prisma": "^5.17.0"
  }
}
